@article {1097,
	title = {A Computational study of a stochastic game based pollution tax model},
	journal = {International Conference on Game Theory, Operations Research and their Applications},
	year = {2012},
	month = {01/2012},
	type = {Paper presentation},
	address = {ISI Chennai},
	abstract = {<p>The pollution tax model given in [1] is formulated as a two player nonzero sum SER-SIT<br />
	(separable reward and state independent transition) discounted stochastic game. We propose<br />
	a variant of pollution tax model which captures more realistic situation than the model given<br />
	in [1]. We model this as a two player nonzero sum discounted stochastic game which does<br />
	not seem to have any special structure. We present some computational results on Nash<br />
	equilibrium of pollution tax model by using our trust region SQP-based algorithm given in<br />
	[2]. We describe some cases where our algorithm converges to only -Nash equilibrium. We<br />
	are able to avoid the situation of -Nash equilibrium by starting our algorithm from different<br />
	initial point. We also establishes the fact that an instance of pollution tax model admits<br />
	a Nash equilibrium which is independent of the discount factor close to 1, an extension of<br />
	Blackwell optimality in Markov decision processes.<br />
	\&nbsp;</p>
	<p>References:</p>
	<p>[1]\&nbsp; J. Filar and K. Vrieze. Competitive Markov Decision Processes. Springer, New York, 1997.<br />
	[2]\&nbsp; V.V. Singh, Nandyala Hemachandra, and K. S. Mallikarjuna Rao. On computing nash equilibrium of stochastic game. Technical report,\&nbsp;\&nbsp;\&nbsp; Industrial Engineering and Operations Research, IIT Bomaby, www.ieor.iitb.ac.in/files/VVS_TR_jan2011.pdf , 2011.<br />
	\&nbsp;</p>},
	url = {http://www.isichennai.com/gtora2012/},
	author = {Vikas Vikram Singh and Nandyala Hemachandra and K. S. Mallikarjuna Rao}
}
@article {ag2012,
	title = {Evolutionary Stability Against Multiple Mutations},
	journal = {Dynamic Games and Applications},
	volume = {2},
	year = {2012},
	note = {<p>10.1007/s13235-012-0051-x</p>},
	month = {12/2012},
	pages = {376-384},
	publisher = {Birkh{\"a}user Boston},
	abstract = {<p>It is known (see, e.g., Weibull in Evolutionary Game Theory, MIT Press, 1995 ) that an evolutionarily stable strategy is not necessarily robust against multiple mutations. Precise definition and analysis of \&ldquo;evolutionarily stable strategy against multiple mutations\&rdquo; are not available in the literature. In this article, we formalize evolutionarily robustness against multiple mutations. Our main result shows that such a robust strategy is necessarily a pure strategy. Further, we study some equivalent formulations and properties of evolutionary stability against multiple mutations. In particular, we characterize completely the robustness against multiple mutations in 2\&times;2 games.</p>},
	keywords = {ESS, Evolutionary game, Multiple mutations, Strict Nash equilibrium},
	issn = {2153-0785},
