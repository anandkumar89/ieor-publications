@article {1894,
	title = {Perspectives on Operations Research (IEOR Seminar Series)},
	year = {2018},
	abstract = {<p>(IEOR Seminar Series)</p>
	<p>Operations research supports decision-making in many complex systems\&nbsp;across domains. However, though OR is a well-established field in its\&nbsp;own right, it is quite unrecognised and under-appreciated. The problem\&nbsp;is compounded by the fact that OR professionals / researchers are\&nbsp;themselves quite unclear about "What is OR?", or unable to explain it\&nbsp;confidently or satisfactorily to the public at large. This vagueness\&nbsp;about OR often poses a barrier for novice learners/ students to join the\&nbsp;OR stream/ employ OR methods effectively. Further, OR community will be\&nbsp;increasingly looked upon to play a key role as an anchor to address\&nbsp;"wicked" problems (incl. sustainable development goals), and steer the\&nbsp;realisation of solutions. This requires us to have a common\&nbsp;understanding about OR, and perhaps introspect our role as OR\&nbsp;professionals.<br />
	<br />
	In this talk I will attempt to provide a satisfactory answer to the\&nbsp;question "What is OR?". Drawing on my own experiences, I argue for a\&nbsp;strong empirical base for research \&amp; practice activities of OR, and the\&nbsp;challenges therein. I also discuss the skill sets required to create<br />
	complete \&amp; holistic OR professionals. This, I believe, will go a long\&nbsp;way in building a strong community of complete OR professionals to\&nbsp;work\&nbsp;on problems that matter .</p>},
	attachments = {https://www.ieor.iitb.ac.in/files/OR-Perspectivesv3.pdf},
	author = {Jayendran Venkateswaran}
}
