@proceedings {1609,
	title = {Facility Location Models for Pallet Supply Chain with end-of-life tracking},
	journal = {Joint International Symposium on CIE{\textquoteright}44 and IMSS{\textquoteright}14},
	year = {2014},
	pages = {558-571},
	abstract = {<p><br />
	Palletization is an essential part of supply chain (SC) logistics. Most often the pallets are provided by a third party, who reuses the pallets across multiple stages of the SC operations in the same or even different SCs. Disruptions in pallet supply will affect the SC operations of the customers. This paper presents a mixed integer linear programming based facility location models for the pallet supplier, given their customer SC networks with demand flows. The key decisions are the location of pallet warehouses which will supply ready-to-use pallets to various customer locations, as well as collect, inspect and repair used pallets from customers. Note that the supply point of pallets to a customer{\textquoteright}s chain may be very different from its point of return, which make the problem non-trivial. The model incorporates multiple trips of pallets as well as end-of-life scenarios. Various operational configurations, such as, strict-return-of-pallet-to-origin-warehouse, supply-and/or-return-of-pallets-to-closest-warehouse, etc. are modeled and analyzed using randomly generated instances. Further the impact of information exchange between the customer SCs and pallet provider is analyzed.<br />
	\&nbsp;</p>
	},
	author = {Ratnaji vanga and Jayendran Venkateswaran and Narayan Rangaraj}
}
@proceedings {1556,
	title = {Iterative Simulation and Optimization Approach for Job Shop Scheduling},
	volume = {2014 Winter Simulation Conference},
	year = {2014},
	month = {12/2014},
	pages = {1620-1631},
	publisher = {IEEE Press},
	abstract = {<p>In this paper, we present an iterative scheme integrating simulation with an optimization model, for solving complex problems, viz., job shop scheduling. The classical job shop scheduling problem which is NP-Hard, has often been modelled as Mixed-Integer Programming (MIP) model and solved using exact algorithms (for example, branch-and-bound and branch-and-cut) or using meta-heuristics (for example, Genetic Algorithm, Particle Swarm Optimization and Simulated Annealing). In the proposed Iterative Simulation-Optimization (ISO) approach, we use a modified formulation of the scheduling problem where the operational aspects of the job shop are captured only in the simulation model. Two new decision variables,\&nbsp;controller delays\&nbsp;and\&nbsp;queue priorities\&nbsp;are used to introduce feedback constraints, that help exchange information between the two models. The proposed method is tested using benchmark instances from the OR library. The results indicate that the method gives near optimal schedules in a reasonable computational time.</p>},
	url = {http://dl.acm.org/citation.cfm?id=2693848.2694054},
	author = {Kulkarni, Ketki and Jayendran Venkateswaran}
}
