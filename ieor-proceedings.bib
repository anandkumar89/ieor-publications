@proceedings {Tammineni20071838,
	title = {Advanced Look-ahead Based Approach (ALBA) for distributed simulation of supply chains},
	journal = {2007 IEEE International Conference on Industrial Engineering and Engineering Management (IEEM 2007)},
	year = {2007},
	note = {<p>cited By (since 1996) 0; Conference of 2007 IEEE International Conference on Industrial Engineering and Engineering Management, IEEM 2007; Conference Date: 2 December 2007 through 4 December 2007; Conference Code: 71532</p>},
	month = {12/2007},
	pages = {1838-1842},
	address = {Singapore},
	abstract = {<p>Distributed simulation is an emerging technique for simulation of supply chains consisting of multiple independent detailed discrete-event models. Time synchronization is one of the critical issues that affect the accuracy of results, network load and execution time. An Advanced Look-ahead Based Approach (ALBA), a hybrid conservative approach for time synchronization, is proposed that allows the models to run as-fast-as-possible to the nearest interaction event. This is achieved using an improved supply chain domain specific look-ahead algorithm that handles multiple types of interactions. The effect of stochasticity of the models on the performance of the look-ahead algorithm is discussed. Also, the performance of ALBA is appraised against two other hybrid time synchronization approaches in literature. Experimental results using a four player distributed supply chain simulation show that ALBA functions better than the other approaches in terms of network communication load and execution time. \&copy; 2007 IEEE.</p>},
	keywords = {Algorithms, Discrete-event, Distributed simulation, Network communication, Supply chain, Synchronization, Time synchronization},
	isbn = {1424415292; 9781424415298},
	doi = {10.1109/IEEM.2007.4419510},
	url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-40649097785\&partnerID=40\&md5=f2871aa1928315dbc34eb29f86b93a47},
	author = {S. Tammineni and Jayendran Venkateswaran}
}
